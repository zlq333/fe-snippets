{
  "name": "fe-snippets",
  "displayName": "fe-snippets",
  "description": "fe-snippets for VS Code",
  "version": "1.0.0",
  "publisher": "zlqqq333",
  "repository": "https://github.com/zlq333/fe-snippets.git",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.gitHubClone",
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.gitHubClone",
        "title": "gitHub clone"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "extension.gitHubClone",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.gitHubClone",
          "group": "navigation"
        }
      ]
    },
    "snippets": [
      {
        "language": "html",
        "path": "./src/snippet/snippets-html.json"
      },
      {
        "language": "typescript",
        "path": "./src/snippet/snippets-ts.json"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "pig-box",
          "title": "pig pig",
          "icon": "./src/images/pig_boss.svg"
        }
      ]
    },
    "views": {
      "pig-box": [
        {
          "id": "pig-1",
          "name": "前端组件库"
        },
        {
          "id": "pig-2",
          "name": "pig-2"
        },
        {
          "id": "pig-3",
          "name": "pig-3"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.32.0",
    "@typescript-eslint/eslint-plugin": "^4.16.0",
    "@typescript-eslint/parser": "^4.16.0",
    "@vscode/test-electron": "^1.6.1",
    "eslint": "^7.21.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "source-map-support": "^0.5.12",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "download-git-repo": "^3.0.2",
    "fs-extra": "^10.0.0"
  }
}
